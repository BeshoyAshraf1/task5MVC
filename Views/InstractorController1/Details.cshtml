@{
}
@model Instractor


<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">image</th>
            <th scope="col">Salary</th>
            <th scope="col">address</th>
            <th scope="col">DepratmentName</th>
            <th scope="col">CourseName</th>
        </tr>
    </thead>
    <tbody>
      
            <tr>
                <td>@Model.Id</td>
            <td>@Model.Name</td>
            <td><img src="~/images/@Model.image" alt="@Model.Name" width="50" /></td>

