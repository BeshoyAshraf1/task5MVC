@{
}
@model Depratment


<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Salary</th>
            <th scope="col">address</th>
        </tr>
    </thead>
    <tbody>

        <tr>
            <td>@Model.id</td>
            <td>@Model.name</td>

            <td>@Model.managername</td>
            <td>
                @foreach (var instructor in Model.Instractors)
                {
                    <li>
                        <strong>Name:</strong> @instructor.Name <br />
                        <strong>Salary:</strong> @instructor.Salary <br />
                        <strong>Address:</strong> @instructor.address
                    </li>
                }
            </td>

        </tr>

    </tbody>
</table>